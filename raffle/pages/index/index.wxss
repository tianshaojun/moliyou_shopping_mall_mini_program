page {
    height: 100%;
    overflow: hidden;
    background: #ff4800;
}

view {
    font-family: PingFang SC,Microsoft YaHei;
}

.hide {
    display: none;
}

.userinfo {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.userinfo-avatar {
    width: 128rpx;
    height: 128rpx;
    margin: 20rpx;
    border-radius: 50%;
}

.userinfo-nickname {
    color: #aaa;
}

.usermotto {
    margin-top: 200rpx;
}

image {
    width: 100%;
    height: auto;
}

.draw {
    width: 610rpx;
    height: 610rpx;
    text-align: center;
    position: absolute;
    margin: 0 auto;
    top: 440rpx;
    left: 70rpx;
}

.draw-bg {
    position: absolute;
    left: 0;
    right: 0;
    top: -5rpx;
    height: 610rpx;
}

.draw .content {
    position: absolute;
    left: 35rpx;
    top: 30rpx;
}

.draw .content image {
    width: 540rpx;
    height: 540rpx;
}

.draw .txt {
    width: 158rpx;
    height: 130rpx;
    color: #fff;
    font-size: 33rpx;
    line-height: 40rpx;
    font-weight: 600;
    padding-top: 55rpx;
    cursor: pointer;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -78rpx;
    margin-top: -96rpx;
}

.draw .txt image {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    width: 150rpx;
    height: 188rpx;
}

.draw .txt text {
    position: relative;
    z-index: 1;
    font-size: 32rpx;
    top: 38rpx;
    left: -6rpx;
    color: #fff;
}

#huojiang {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0,0,0,0.6);
    z-index: 999;
}

#huojiang .inner {
    width: 640rpx;
    height: 640rpx;
    margin: 150rpx auto;
    position: relative;
}

.huojiang-close {
    display: inline-block;
    width: 54rpx;
    height: 54rpx;
    cursor: pointer;
    position: absolute;
    top: 77rpx;
    right: 69rpx;
}

.huojiang-know {
    width: 190rpx;
    height: 50rpx;
    line-height: 50rpx;
    color: #fff;
    text-align: center;
    display: inline-block;
    position: absolute;
    bottom: 73rpx;
    right: 50%;
    margin-right: -95rpx;
    font-size: 26rpx;
    cursor: pointer;
}

#huojiang .content {
    width: 413rpx;
    font-size: 36rpx;
    line-height: 54rpx;
    color: #fff;
    position: absolute;
    bottom: 50%;
    right: 50%;
    margin-right: -205rpx;
    margin-bottom: -32rpx;
    text-align: center;
}

#huojiang .content span {
    display: block;
}

.change-code {
    width: 338rpx;
    height: 36rpx;
    background: #c9101a;
    color: #fff;
    font-size: 12rpx;
    line-height: 36rpx;
    position: absolute;
    bottom: 50%;
    right: 50%;
    margin-right: -169rpx;
    margin-bottom: -72rpx;
    text-align: center;
}

.kufu {
    width: 218rpx;
    height: 36rpx;
    color: #fff;
    padding: 0 60rpx;
    font-size: 20rpx;
    line-height: 36rpx;
    position: absolute;
    bottom: 50%;
    right: 50%;
    margin-right: -169rpx;
    margin-bottom: -108rpx;
    text-align: center;
}

.raffle {
    height: 498rpx;
    width: 572rpx;
    overflow: hidden;
    position: absolute;
    margin: 0 auto;
    top: 525rpx;
    left: 93rpx;
}

.raffle:after {
    clear: both;
    content: '.';
    display: block;
    width: 0;
    height: 0;
    visibility: hidden;
}

.raffle .raffle-img {
    position: absolute;
    top: 0;
    left: 0;
    height: 100%;
}

#lottery {
    height: 478rpx;
    width: 556rpx;
    margin: 10rpx auto 0;
}

#lottery view {
    position: relative;
    width: 174rpx;
    margin-right: 16rpx;
    height: 155.3rpx;
    float: left;
    margin-bottom: 4px;
}

#lottery view.lottery-unit:nth-child(3n) {
    margin-right: 0;
}

#lottery view a {
    width: 172rpx;
    height: 155.3rpx;
    display: block;
    margin-right: 2rpx;
    text-decoration: none;
    text-align: center;
    line-height: 155.3rpx;
    color: #fff;
    font-size: 44rpx;
}

#lottery view.active .mask {
    display: block;
}

#lottery view .mask {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    background: rgba(255,238,134,0.5);
    display: none;
}

#mask {
    background: rgba(0,0,0,0.65);
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 2;
}

.model {
    position: fixed;
    top: 200rpx;
    left: 60rpx;
    right: 60rpx;
    padding: 40rpx;
    background: #fff;
    border-radius: 8rpx;
    z-index: 2;
    text-align: center;
    min-height: 440rpx;
    max-height: 800rpx;
    box-sizing: border-box;
}

.model.prize_show {
    padding: 40rpx 0;
}

.model .model-close {
    position: absolute;
    top: -18rpx;
    right: -12rpx;
    width: 56rpx;
}

.model.prize_show .model-close {
    right: 18rpx;
}

.model .title-img {
    position: absolute;
    width: 400rpx;
    left: 73rpx;
}

.model.prize_show .title-img {
    left: 106rpx;
}

.model .coupon-img {
    width: 563rpx;
    margin-bottom: 40rpx;
    box-shadow: 6rpx 6rpx 29rpx #ddd;
}

.model .title {
    font-size: 40rpx;
    color: #333;
    font-weight: bold;
    margin-bottom: 40rpx;
}

.model .subtitle {
    font-size: 28rpx;
    color: #333;
    line-height: 76rpx;
}

.model .title span {
    color: #ffae00;
    font-size: 1.2rem;
    vertical-align: bottom;
    margin-left: 0.2rem;
}

.model .prize {
    width: 10.75rem;
    height: 2.75rem;
    line-height: 2.75rem;
    font-size: 0.85rem;
    color: #fff;
    background: #f4361e;
    margin: 0 auto;
    display: block;
    text-align: center;
}

.model .prize span {
    font-size: 1.55rem;
    margin-right: 0.4rem;
}

.model .cdkey {
    height: 54rpx;
    line-height: 54rpx;
    color: #333;
    font-size: 22rpx;
    text-align: center;
    border-radius: 27rpx;
    background: #fff2eb;
}

.model .copy {
    width: 200rpx;
    height: 60rpx;
    background: #ff5931;
    line-height: 60rpx;
    color: #fff;
    font-size: 26rpx;
    margin: 30rpx auto 20rpx;
    border-radius: 30rpx;
}

.model .time {
    font-size: 20rpx;
    color: #999;
}

.model .hint {
    color: #666;
    font-size: 26rpx;
}

.model .know {
    width: 5.9rem;
    height: 1.5rem;
    line-height: 1.5rem;
    background: #f4361e;
    margin: 0 auto;
    border-radius: 0.45rem;
    color: #fff;
    text-align: center;
    font-size: 0.8rem;
}

.raffle-bottom {
    position: absolute;
    top: 1230rpx;
    width: 100%;
    text-align: center;
}

.raffle-bottom>view {
    display: inline-block;
    position: relative;
    width: 281rpx;
    height: 71rpx;
}

.residue {
    position: absolute;
    top: 465rpx;
    width: 100%;
    text-align: center;
    color: #c54e16;
    font-size: 24rpx;
}

.draw-top {
    position: absolute;
    top: 384rpx;
    width: 100%;
    text-align: center;
}

.draw-top>view {
    display: inline-block;
    position: relative;
    width: 157rpx;
    height: 78rpx;
    line-height: 85rpx;
    color: #fff;
    font-size: 28rpx;
    overflow: hidden;
}

.draw-bottom {
    position: absolute;
    top: 1191rpx;
    width: 100%;
    text-align: center;
}

.draw-bottom>view {
    display: inline-block;
    position: relative;
    width: 300rpx;
    height: 90rpx;
    line-height: 90rpx;
    color: #fe6100;
    font-size: 28rpx;
}

.raffle-bottom .raffle-btn {
    width: 281rpx;
    height: 71rpx;
    line-height: 71rpx;
    font-size: 30rpx;
    color: #ff5931;
    border-radius: 35rpx;
    background: #ffece8;
}

.raffle-bottom .raffle-btn.raffle-btn-b {
    background: #e8bcb2;
    position: absolute;
    bottom: -6rpx;
    z-index: 0;
}

.raffle-bottom .raffle-btn.raffle-btn-t {
    position: absolute;
    z-index: 1;
}

.circulation {
    position: absolute;
    top: 1070rpx;
    width: 516rpx;
    left: 139rpx;
    color: #c2541a;
    font-size: 24rpx;
}

.rule {
    background: #ef461b;
    padding: 150rpx 60rpx 0;
}

.rule>view {
    background: #fffcf5;
    border-radius: 20rpx;
    position: relative;
    padding: 30rpx;
    padding-bottom: 70rpx;
}

.rule .rule-bg1 {
    position: absolute;
    top: 40rpx;
    width: 300rpx;
    left: 50%;
    margin-left: -150rpx;
}

.rule .rule-bg2,.prize-details .rule-bg2,.award-rule .rule-bg2 {
    position: absolute;
    bottom: 12rpx;
    width: auto;
    left: 12rpx;
    right: 12rpx;
}

.prize-details .rule-bg2 {
    bottom: -100rpx;
}

.award-rule .rule-bg2 {
    bottom: -60rpx;
}

.rule .rule-title {
    color: #fc5b2f;
    font-size: 34rpx;
    text-align: center;
    padding-top: 10rpx;
    padding-bottom: 40rpx;
}

.rule .rule-span {
    height: 44rpx;
    width: 148rpx;
    line-height: 44rpx;
    text-align: center;
    color: #fff;
    background: #ff5931;
    border-radius: 22rpx;
    font-size: 26rpx;
    display: inline-block;
    margin: 10rpx 0;
}

.rule>view>view {
    font-size: 24rpx;
    line-height: 44rpx;
    margin: 0 10rpx 14rpx;
}

swiper .wx-swiper-dots {
    display: none;
}

.prize_show .prize-content {
    max-height: 310px;
    overflow-y: auto;
    min-height: 260rpx;
    padding: 0 32rpx;
}

.prize_show .prize-child {
    box-shadow: 6rpx 6rpx 29rpx #ddd;
    margin: 40rpx 0;
    height: 290rpx;
    padding: 0 20rpx;
}

.prize-details,.award-rule {
    height: 700rpx;
    padding: 40rpx 26rpx 80rpx;
    overflow: auto;
}

.prize-details .title,.award-rule .title {
    color: #ff5931;
    margin-bottom: 50rpx;
}

.award-rule .prize-details-content {
    text-align: left;
}

.prize-details-content>view {
    font-size: 24rpx;
    color: #333;
    line-height: 50rpx;
    text-align: left;
    display: flex;
}

.prize-details-content>view>view:first-of-type {
    width: 106rpx;
}

.prize-details-content>view>view:last-of-type {
    flex: 1;
}

.prize-details-content>view text {
    color: #ff5931;
}

.award-rule .rule-span {
    height: 44rpx;
    width: 148rpx;
    line-height: 44rpx;
    text-align: center;
    color: #fff;
    background: #ff5931;
    border-radius: 22rpx;
    font-size: 26rpx;
    display: inline-block;
    margin: 10rpx 0;
}

.award-rule .prize-details-content>view {
    font-size: 24rpx;
    line-height: 44rpx;
    margin: 0 10rpx 14rpx;
}

.change {
    position: fixed;
    width: 135rpx;
    height: 60rpx;
    background: #333;
    color: #fff;
    font-size: 26rpx;
    text-align: center;
    line-height: 60rpx;
    right: 40rpx;
    bottom: 170rpx;
    border-radius: 10rpx;
    z-index: 2;
}

.register,.login {
    position: fixed;
    top: 260rpx;
    left: 62rpx;
    right: 62rpx;
    background: #fff;
    width: auto;
    border-radius: 10rpx;
    padding: 0 12rpx;
    z-index: 2;
}

.register .title,.login .title {
    line-height: 112rpx;
    height: 112rpx;
    text-align: center;
    color: #333333;
    font-size: 32rpx;
    border-bottom: 1rpx solid #efefef;
}

.register .form,.login .form {
    padding: 22rpx 32rpx 84rpx;
    overflow: hidden;
}

.register .form .note,.register .form .tel,.register .form .password,.login .form .tel,.login .form .password {
    border: 1rpx solid #e7e6e6;
    height: 86rpx;
    line-height: 86rpx;
    margin-bottom: 20rpx;
    padding-left: 16rpx;
    box-sizing: border-box;
    float: left;
    width: 100%;
}

.register .form view input,.login .form view input {
    border: 0;
    font-size: 24rpx;
    vertical-align: text-bottom;
    line-height: 84rpx;
    height: 84rpx;
}

.register .form .note,.register .form .verification {
    width: 296rpx;
    margin-right: 20rpx;
}

.register .form .note input,.register .form .verification input {
    width: 200rpx;
}

.register .form .icox,.login .form .icox {
    font-size: 42rpx;
    color: #cdcdcd;
    margin-right: 12rpx;
    float: left;
}

.sendNote {
    display: inline-block;
    width: 220rpx;
    height: 88rpx;
    line-height: 88rpx;
    background: #44abf7;
    color: #fff;
    font-size: 24rpx;
    text-align: center;
    float: left;
    text-decoration: none;
    border-radius: 4rpx;
}

.sendNote.disabled {
    background: #ccc;
}

.register .form image {
    width: 186rpx;
    height: 60rpx;
    float: left;
    margin-top: 20rpx;
}

.register .submit,.login .submit {
    float: left;
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    color: #fff;
    background: #44abf7;
    text-align: center;
    font-size: 28rpx;
    border-radius: 4rpx;
    border: 0;
}

.register .close,.login .close {
    position: absolute;
    left: 280rpx;
    bottom: -96rpx;
    font-size: 68rpx;
    color: #fff;
}

.login .bottom {
    background: #fff;
    color: #00aeff;
    font-size: 28rpx;
    text-align: center;
    margin-top: 40rpx;
    float: left;
}

.login .bottom a {
    color: #00aeff;
    text-decoration: underline;
}